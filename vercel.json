{
  "version": 2,
  "builds": [
    {
      "src": "/backend/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      
      "src": "/api/(.*)",
      "dest": "/backend/src/server.js"
    },
    {
      "src": "/swagger/(.*)", 
      "dest": "/backend/src/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/src/server.js"
    }
  ]
}

